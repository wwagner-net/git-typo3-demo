<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file source-language="en" datatype="plaintext" original="EXT:workspaces/Resources/Private/Language/locallang_mod.xlf" date="2011-10-17T20:22:38Z" product-name="workspaces" target-language="de">
    <header/>
    <body>
      <trans-unit id="mlang_tabs_tab" resname="mlang_tabs_tab" approved="yes">
        <source>Workspaces</source>
        <target state="final">Arbeitsumgebungen</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tabdescr" resname="mlang_labels_tabdescr" approved="yes">
        <source>This module contains the overview of all elements within the current workspace and it enables to continue the review and publishing workflow for them.</source>
        <target state="final">Dieses Modul stellt eine Übersicht aller Elemente der aktuellen Arbeitsumgebung dar. Es ermöglicht darüber hinaus Arbeitsschritte zur Überprüfung und Veröffentlichung der Elemente durchzuführen.</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tablabel" resname="mlang_labels_tablabel" approved="yes">
        <source>Create and handle versioning workflows</source>
        <target state="final">Arbeitsumgebungen erstellen und verwalten</target>
      </trans-unit>
      <trans-unit id="stage_editing" resname="stage_editing" approved="yes">
        <source>Editing</source>
        <target state="final">Bearbeitung</target>
      </trans-unit>
      <trans-unit id="stage_ready_to_publish" resname="stage_ready_to_publish" approved="yes">
        <source>Ready to publish</source>
        <target state="final">Bereit zum Veröffentlichen</target>
      </trans-unit>
      <trans-unit id="stage_publish" resname="stage_publish" approved="yes">
        <source>Publish</source>
        <target state="final">Veröffentlichen</target>
      </trans-unit>
      <trans-unit id="stage_all" resname="stage_all" approved="yes">
        <source>All stages</source>
        <target state="final">Alle Stufen</target>
      </trans-unit>
      <trans-unit id="previewText" resname="previewText" approved="yes">
        <source>Preview of workspace %s (%s)</source>
        <target state="final">Vorschau von Arbeitsumgebung %s (%s)</target>
      </trans-unit>
      <trans-unit id="stopPreview" resname="stopPreview" approved="yes">
        <source>Stop preview</source>
        <target state="final">Vorschau stoppen</target>
      </trans-unit>
      <trans-unit id="previewLogoutSuccess" resname="previewLogoutSuccess" approved="yes">
        <source>You logged out from Workspace preview mode. Click this link to %1$s go back to the website%2$s</source>
        <target state="final">Sie haben den Arbeitsumgebungsvorschaumodus verlassen. Folgen Sie diesem Link um %1$s zur Website zurückzugehen%2$s</target>
      </trans-unit>
      <trans-unit id="previewLogoutError" resname="previewLogoutError" approved="yes">
        <source>%1$sERROR!%2$sTemplate File "%3$s" configured with $TYPO3_CONF_VARS["FE"]["workspacePreviewLogoutTemplate"] not found. Please contact webmaster about this problem.</source>
        <target state="final">%1$sFEHLER!%2$sTemplatedatei "%3$s" konfiguriert mit $TYPO3_CONF_VARS["FE"]["workspacePreviewLogoutTemplate"] nicht gefunden. Bitte kontaktieren Sie Ihren Webmaster wegen dieses Problems.</target>
      </trans-unit>
    </body>
  </file>
